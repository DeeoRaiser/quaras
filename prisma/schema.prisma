  pago_compra_id    Int?
  trial260          String?           @db.Char(1)
  cuentas_bancarias cuentas_bancarias @relation(fields: [banco_id], references: [id], onUpdate: Restrict, map: "movimientos_bancarios_ibfk_1")
  usuarios          usuarios          @relation(fields: [user_id], references: [id], onUpdate: Restrict, map: "movimientos_bancarios_ibfk_2")

  @@index([banco_id], map: "cuenta_bancaria_id")
  @@index([user_id], map: "idx_user_id")
}

model pagos {
  id                 Int                  @id(map: "pk_pagos") @default(autoincrement())
  cliente_id         Int
  fecha              DateTime             @db.Date
  monto              Decimal              @db.Decimal(12, 4)
  metodo             String               @db.VarChar(13)
  banco_id           Int?
  lote               String?              @db.VarChar(50)
  cupon              String?              @db.VarChar(50)
  tarjeta            String?              @db.VarChar(50)
  observacion        String?
  comprobante        String?              @db.VarChar(50)
  created_at         DateTime             @default(now()) @db.Timestamp(6)
  updated_at         DateTime             @default(now()) @db.Timestamp(6)
  trial035           String?              @db.Char(1)
  aplicaciones_pagos aplicaciones_pagos[]
  clientes           clientes             @relation(fields: [cliente_id], references: [id], onUpdate: Restrict, map: "pagos_ibfk_1")
  bancos             bancos?              @relation(fields: [banco_id], references: [id], onDelete: Restrict, onUpdate: Restrict, map: "pagos_ibfk_2")

  @@index([banco_id], map: "idx_banco_id")
  @@index([cliente_id], map: "idx_cliente_id")
}

model pagos_compras {
  id                         Int                          @id(map: "pk_pagos_compras") @default(autoincrement())
  proveedor_id               Int
  factura_id                 Int
  fecha                      DateTime                     @db.Timestamp(6)
  monto                      Decimal                      @db.Decimal(12, 2)
  metodo                     String                       @db.VarChar(13)
  banco                      String?                      @db.VarChar(100)
  lote                       String?                      @db.VarChar(50)
  cupon                      String?                      @db.VarChar(50)
  tarjeta                    String?                      @db.VarChar(50)
  observacion                String?
  comprobante                String?                      @db.VarChar(100)
  created_at                 DateTime                     @default(now()) @db.Timestamp(6)
  updated_at                 DateTime                     @default(now()) @db.Timestamp(6)
  trial077                   String?                      @db.Char(1)
  aplicaciones_pagos_compras aplicaciones_pagos_compras[]
  factura_compras            factura_compras              @relation(fields: [factura_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "fk_pc_factura")

  @@index([factura_id], map: "idx_pc_factura")
  @@index([proveedor_id], map: "idx_pc_proveedor")
}

model proveedor_articulos {
  id                Int               @id(map: "pk_proveedor_articulos") @default(autoincrement())
  proveedor_id      Int
  articulo_id       Int
  precio_compra     Decimal?          @db.Decimal(12, 2)
  codigo_proveedor  String?           @db.VarChar(50)
  activo            Int?              @default(1) @db.SmallInt
  trial266          String?           @db.Char(1)
  proveedores       proveedores       @relation(fields: [proveedor_id], references: [id], onUpdate: Restrict, map: "proveedor_articulos_ibfk_1")
  articulos_compras articulos_compras @relation(fields: [articulo_id], references: [id], onUpdate: Restrict, map: "proveedor_articulos_ibfk_2")

  @@unique([proveedor_id, articulo_id], map: "uk_proveedor_articulo")
  @@index([articulo_id], map: "idx_articulo_id")
}

model proveedores {
  id                  Int                   @id(map: "pk_proveedores") @default(autoincrement())
  nombre              String                @db.VarChar(255)
  razonsocial         String                @db.VarChar(100)
  iva                 String?               @db.VarChar(15)
  iibb                String?               @db.VarChar(19)
  numiibb             String?               @db.VarChar(14)
  cuit                String?               @db.VarChar(11)
  direccion           String?               @db.VarChar(255)
  telefono            String?               @db.VarChar(50)
  email               String?               @db.VarChar(150)
  nota                String?
  created_at          DateTime              @default(now()) @db.Timestamp(6)
  updated_at          DateTime              @default(now()) @db.Timestamp(6)
  trial061            String?               @db.Char(1)
  factura_compras     factura_compras[]
  proveedor_articulos proveedor_articulos[]
}

model puntos_venta {
  id               Int      @id(map: "pk_puntos_venta") @default(autoincrement())
  usuario_id       Int
  nombre           String   @db.VarChar(100)
  punto_venta      Int
  numero           Int
